{
  "name": "halgai_api",
  "version": "1.0.0",
  "private": true,
  "license": "Suchin Mende.",
  "scripts": {
    "start": "node ./bin/www",
    "stop": "pm2 stop ecosystem.config.js",
    "startDev": "tsc & pm2 start ecosystem.config.js",
    "build": "tsc",
    "pm2": "pm2 start ecosystem.config.js",
    "pm2Stage": "pm2 start ecosystem.config.js --env stage",
    "pm2Production": "pm2 start ecosystem.config.js --env production",
    "restart": "pm2 reload ecosystem.config.js",
    "restartStage": "pm2 reload ecosystem.config.js --env stage",
    "restartProduction": "pm2 reload ecosystem.config.js --env production",
    "dev": "tsc --allowJs --watch & NODE_ENV=development nodemon ./bin/www",
    "local": "tsc --allowJs --watch & NODE_ENV=local nodemon --inspect ./bin/www",
    "test": "tsc && mocha dist/**/*.spec.js",
    "lint": "eslint src --ext ts",
    "tsc": "tsc"
  },
  "dependencies": {
    "aws-sdk": "^2.694.0",
    "axios": "^0.19.2",
    "bcrypt": "^5.0.0",
    "bluebird": "^3.7.2",
    "body-parser": "~1.19.0",
    "camelcase-keys-deep": "^0.1.0",
    "connect-redis": "^3.4.2",
    "cookie": "^0.4.1",
    "cookie-parser": "^1.4.5",
    "cookie-signature": "^1.1.0",
    "cors": "^2.8.5",
    "crc": "^3.8.0",
    "debug": "~4.1.1",
    "depd": "^2.0.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.6",
    "fs": "0.0.1-security",
    "images": "^3.2.3",
    "moment": "^2.26.0",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.4",
    "on-headers": "^1.0.2",
    "orm": "^5.0.5",
    "parseurl": "^1.3.3",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pug": "~3.0.0",
    "qrcode": "^1.4.4",
    "redis": "^3.0.2",
    "rotating-file-stream": "^2.1.1",
    "serve-favicon": "~2.5.0",
    "snake-case": "^3.0.3",
    "uid-safe": "^2.1.5",
    "winston": "^2.4.4",
    "winston-daily-rotate-file": "^3.10.0",
    "xlsx": "^0.16.2"
  },
  "description": "Halgai api",
  "main": "app.js",
  "author": "Suchin Mende",
  "devDependencies": {
    "@types/method-override": "0.0.31",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.13",
    "@types/underscore": "^1.10.7",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-promise": "^4.2.1",
    "mocha": "^8.0.1",
    "supertest": "^4.0.2",
    "tsc": "^1.20150623.0",
    "typescript": "^3.9.5",
    "typescript-eslint-parser": "^22.0.0"
  }
}
