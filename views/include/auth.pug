.panel.panel-default
  .panel-heading
    h5.panel-title
      a(data-toggle='collapse', href='#auth')
        h1 認証系処理
  #auth.panel-collapse.collapse
    .panel-body
      .panel.panel-default
        .panel-heading
          h5.panel-title ログインテスト
        .panel-body
          form(method='post', class='form-horizontal', id='login-form')
            .form-group.form-group-lg
              label(for='userCd', class='control-label col-sm-4') ユーザCD
              .col-sm-8
                input(type='text', name='userCd', value='', id='userCd', class='form-control', placeholder='ユーザCD')
            .form-group.form-group-lg
              label(for='password', class='control-label col-sm-4') パスワード
              .col-sm-8
                input(type='password', name='password', value='', id='password', class='form-control', placeholder='パスワード')
            .form-group.form-group-lg
              label(for='apiKey', class='control-label col-sm-4') APIキー
              .col-sm-8
                input(type='text', name='apiKey', value='', id='apiKey', class='form-control', placeholder='APIキー', disabled)
            <br>
            button(type='button', id='login', class='btn btn-primary btn-block btn-lg')
              span(class='glyphicon glyphicon-log-in', aria-hidden='true')
              span ログイン
            button(type='button', id='login-clear', class='btn btn-lg btn-block btn-default') クリア
            <hr>
            .form-group.form-group-lg
              label(for='login-result', class='control-label col-sm-4') ログイン結果
              .col-sm-8
                textarea(id='login-result', class='form-control', placeholder='ログイン結果', disabled, rows='10')

      .panel.panel-default
        .panel-heading
          h5.panel-title ログアウトテスト
        .panel-body
          form(method='post', class='form-horizontal', id='logout-form')
            .form-group.form-group-lg
              label(for='logoutApiKey', class='control-label col-sm-4') APIキー
              .col-sm-8
                input(type='text', name='apiKey', value='', id='logoutApiKey', class='form-control', placeholder='APIキー', disabled)
            .form-group.form-group-lg
